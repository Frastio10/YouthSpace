<div class="popup "   id="popup-success">
	<div class="popup-parent">
		<div class="popup-content  ">
			<div class="popup-report  background-white">
				<div class=" popup-node  background-white">
					<div class="pg-20 wd-100-p   center-report">
						<h1 class="text-align-center font-light color-dark">
						Coupon Successfully Used
						</h1>
						<div class="wd-100-p flex justify-content-center mg-t-30">
							<button class="button button-line-primary close-popup pg-x-40 rounded-50 color-primary">CLOSE</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="popup "   id="popup-fail">
	<div class="popup-parent">
		<div class="popup-content  ">
			<div class="popup-report  background-white">
				<div class=" popup-node  background-white">
					<div class="pg-20 wd-100-p   center-report">
						<h1 class="text-align-center font-light color-dark">
						Coupon Failed to Enter / Ever Used
						</h1>
						<div class="wd-100-p flex justify-content-center mg-t-30">
							<button class="button button-line-primary close-popup pg-x-40 rounded-50 color-primary">CLOSE</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>